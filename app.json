{
	"expo": {
		"name": "Contagiante",
		"slug": "metocontagiante-mobile",
		"platforms": ["ios", "android"],
		"version": "1.1.4",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "contagiante",
		"primaryColor": "#000000",
		"privacy": "public",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#000000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"icon": "./assets/images/iosicon.png",
			"bundleIdentifier": "br.com.metodistacontagiante.metocontagiante",
			"buildNumber": "1.1.4"
		},
		"android": {
			"package": "br.com.andresouzadev.metocontagiante",
			"versionCode": 4,
			"permissions": [
				"ACCESS_COARSE_LOCATION",
				"ACCESS_FINE_LOCATION",
				"MANAGE_DOCUMENTS",
				"VIBRATE"
			],
			"googleServicesFile": "./google-services.json"
		},
		"notification": {
			"icon": "./assets/images/icon96x96.png",
			"color": "#000000",
			"iosDisplayInForeground": true,
			"androidMode": "default"
		}
	}
}
